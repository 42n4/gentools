for i in `chroot-armv7a "$@" 2>&1 | grep "\[ebuild" | cut -d: -f1 | cut -d] -f2 | sed 's/\ /=/'`; do armv7a-hardfloat-linux-gnueabi-emerge --config-root=/usr/armv7a-hardfloat-linux-gnueabi/ --nodeps $i; if [ "$?" = "0" ]; then echo $i >> /var/tmp/packages_arm/packages_done.txt; else echo $i >> /var/tmp/packages_arm/packages_todo.txt; fi;  done

